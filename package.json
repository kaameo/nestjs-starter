    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "prettier": "^3.8.1",
    "supertest": "^7.2.2",
    "testcontainers": "^11.11.0",
    "typescript": "^5.9.3",
    "unplugin-swc": "^1.5.9",
